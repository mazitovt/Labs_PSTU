@model MilitaryDistrictAPI.Models.scaffolded.VehicleCategory

@{
    ViewData["Title"] = "Категория техники";
    Layout = "~/Views/Shared/CRUD/_LayoutEdit.cshtml";
}

<input type="hidden" asp-for="VehicleCategoryId" />
<div class="form-group">
    <label asp-for="UnitId" class="control-label"></label>
    <select asp-for="UnitId" class="form-control" asp-items="ViewBag.UnitId"></select>
    <span asp-validation-for="UnitId" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
            
