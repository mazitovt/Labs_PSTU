@model MilitaryDistrictAPI.Models.scaffolded.Building

@{
    Layout = "CRUD/_LayoutCreate";
    ViewData["Title"] = "Сооружение";
}

<div class="form-group">
    <label asp-for="UnitId" class="control-label"></label>
    <select asp-for="UnitId" class ="form-control" asp-items="ViewBag.UnitId"></select>
</div>
<div class="form-group">
    <label asp-for="NumberOfLivingSubunits" class="control-label"></label>
    <input asp-for="NumberOfLivingSubunits" class="form-control" />
    <span asp-validation-for="NumberOfLivingSubunits" class="text-danger"></span>
</div>

       
